    "import itertools\n",
    "def get_permutations(s, n):\n",
    "    ans = itertools.permutations(s,n)\n",
    "    ans2 = [''.join(a) for a in ans]\n",
    "    print(ans2)\n",
    "    ans2.sort()\n",
    "    return list(ans2)\n",
    "    \n",
    "    \n",
    "\n",
    "get_permutations(\"cat\", 2) == [\"ac\", \"at\", \"ca\", \"ct\", \"ta\", \"tc\"]"
   ]

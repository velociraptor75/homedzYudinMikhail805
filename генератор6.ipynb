    "import itertools\n",
    "def maximize(lists, m):\n",
    "    mx = itertools.starmap(max, lists)\n",
    "    x = 0\n",
    "    for k in mx:\n",
    "        x += k**2\n",
    "    return x % m\n",
    "\n",
    "lists = [\n",
    "    [5, 4,-7],\n",
    "    [7, 8, 9],\n",
    "    [5, 7, 8, 9, 10]\n",
    "]\n",
    "print(maximize(lists,1000))"
   ]
